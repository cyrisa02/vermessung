<form method="post" action="{{ path('app_quotation_delete', {'id': quotation.id}) }}" onsubmit="return confirm('Wollen Sie den Preisangebot löschen?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ quotation.id) }}">
	<button class="btn btn-danger btn-lg mt-4">Löschen</button>

</form>
